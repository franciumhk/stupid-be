{
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "buildCommand": "pip install -r requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.py"
      }
    ]
  }
  